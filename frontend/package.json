{
  "name": "encam",
  "version": "0.1",
  "description": "Encyclopedia of the CAncer Microenvironment frontend",
  "repository": "git@github.com:NBISweden/encam.git",
  "author": "NBIS",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.10.4",
    "@snowpack/app-scripts-react": "^1.10.0",
    "@snowpack/plugin-webpack": "^2.0.12",
    "@testing-library/react": "^10.0.4",
    "@types/jest": "^25.2.2",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.9.5",
    "@types/react-test-renderer": "^16.9.3",
    "@types/snowpack-env": "^2.3.0",
    "babel-jest": "^26.0.1",
    "babel-plugin-emotion": "^10.0.33",
    "canvas": "^2.6.1",
    "colorthief": "^2.3.0",
    "doctest-ts": "^0.5.0",
    "esbuild": "0.6.27",
    "jest": "^26.0.1",
    "jest-emotion": "^10.0.32",
    "path": "^0.12.7",
    "prettier": "^2.1.1",
    "react-test-renderer": "^16.13.1",
    "snowpack": "^2.10.3",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "start": "snowpack dev",
    "build": "./build.sh",
    "pretty": "yarn run prettier --list-different --write src test",
    "typecheck": "find src test -name '*ts*' | xargs tsc --noEmit --jsx react --lib es2019,dom --skipLibCheck --esModuleInterop --module esnext --moduleResolution node",
    "doctests:gen": "find src test -name '*ts*' | xargs doctest-ts --jest",
    "doctests:watch": "find src test -name '*ts*' | entr -c sh -c 'yarn run doctests:gen && yarn run jest src --verbose --testEnvironment node'",
    "test": "yarn run doctests:gen && jest"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@types/react-router-dom": "^5.1.5",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-is": ">= 16.8.0",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.2.0",
    "vega": "^5.10.0",
    "vega-lite": "^4.7.0",
    "vega-tooltip": "^0.22.1"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "jsxBracketSameLine": true,
    "bracketSpacing": false,
    "arrowParens": "avoid",
    "embeddedLanguageFormatting": "off"
  }
}
